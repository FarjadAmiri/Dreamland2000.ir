@model WebSite.DataLayer.Entities.Contact.Contact

<section class="bg-danger call_action_wrap-wrap">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
							
                <div class="call_action_wrap">
                    <div class="call_action_wrap-head">
                        <h3>
                            ¿Tienes preguntas?
                        </h3>
                        <span>
                            Los expertos y consultores de Dreamland están listos para responderle.
                        </span>
                    </div>
                    <a asp-controller="Contact" asp-action="Index" class="btn btn-call_action_wrap">
                        Contacta con nosotras hoy
                    </a>
                </div>
							
            </div>
        </div>
    </div>
</section>