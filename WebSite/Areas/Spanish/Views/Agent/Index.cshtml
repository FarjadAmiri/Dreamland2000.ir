@model IEnumerable<WebSite.DataLayer.Entities.Agent.Agent>
@{
    ViewData["Title"] = "Dreamland 2000 Agents";
    ViewData["MetaDescription"] = "Dreamland 2000 with a team of specialists and experienced experts";
}

<div class="page-title" style="background:#f4f4f4 url(/english/img/slider-1.jpg);" data-overlay="5">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12">
							
                <div class="breadcrumbs-wrap position-relative z-1">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item active" aria-current="page">
                            All Agents 
                        </li>
                    </ol>
                    <h2 class="breadcrumb-title">
                        Agents - Meet All Agents
                    </h2>
                </div>
							
            </div>
        </div>
    </div>
</div>


<section class="gray min">
	<div class="container">

		<div class="row">

            @foreach (var item in Model)
            {
                <div class="col-xl-3 col-lg-3 col-md-6 col-sm-6">
                    <div class="grid_agents style-2">
                       
                        <div class="grid_agents-wrap">

                            <div class="fr-grid-thumb">
                                <a asp-controller="Agent" asp-action="Details" asp-route-id="@item.Id" title="@item.Name @item.Responsibility">
                                    <img src="/upload/agent/@item.Photo" class="img-fluid mx-auto" alt="@item.Name @item.Responsibility">
                                </a>
                                <ul class="inline_social">
                                    <li>
                                        <a asp-controller="Agent" asp-action="Details" asp-route-id="@item.Id" title="@item.Name @item.Responsibility">
                                            <i class="ti-facebook"></i>
                                        </a>
                                    </li>
                                    <li>
                                        <a asp-controller="Agent" asp-action="Details" asp-route-id="@item.Id" title="@item.Name @item.Responsibility">
                                            <i class="ti-linkedin"></i>
                                        </a>
                                    </li>
                                    <li>
                                        <a asp-controller="Agent" asp-action="Details" asp-route-id="@item.Id" title="@item.Name @item.Responsibility">
                                            <i class="ti-instagram"></i>
                                        </a>
                                    </li>
                                    <li>
                                        <a asp-controller="Agent" asp-action="Details" asp-route-id="@item.Id" title="@item.Name @item.Responsibility">
                                            <i class="ti-twitter"></i>
                                        </a>
                                    </li>
                                </ul>
                            </div>

                            <div class="fr-grid-deatil">
                                <span>
                                    <i class="ti-location-pin me-1"></i>
                                    Malaga,Spain
                                </span>
                                <h5 class="fr-can-name">
                                    <a asp-controller="Agent" asp-action="Details" asp-route-id="@item.Id" title="@item.Name @item.Responsibility">
                                        @item.Name
                                    </a>
                                </h5>
                            </div>

                            <div class="fr-infos-deatil">
                                <a asp-controller="Agent" asp-action="Details" asp-route-id="@item.Id" title="@item.Name @item.Responsibility" class="btn btn-danger">
                                    <i class="fa fa-envelope me-2"></i>
                                    Message
                                </a>
                                <a asp-controller="Agent" asp-action="Details" asp-route-id="@item.Id" title="@item.Name @item.Responsibility" class="btn btn-dark">
                                    <i class="fa fa-phone"></i>
                                </a>
                            </div>

                        </div>

                    </div>
                </div>
            }

		</div>

	</div>

</section>
