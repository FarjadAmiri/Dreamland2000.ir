<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta name="robots" content="index, follow">

<meta name="keywords" content="دریملند پرتال و رسانه تخصصی  شهرستان کازرون در حوزه های مختلف شامل  بانک های اطلاعاتی جامع  مورد نیاز شهروندان شهرستان کازرون است">

@if (ViewData["MetaAuthor"] != null)
{
    <meta name="author" content="@ViewData["MetaAuthor"]">
}
else
{
    <meta name="author" content="دریملند 2000">
}

@if (ViewData["MetaDescription"] != null)
{
    <meta name="description" content="@ViewData["MetaDescription"]">
}
else
{
    <meta name="description" content="دریملند پرتال و رسانه تخصصی  شهرستان کازرون در حوزه های مختلف شامل  بانک های اطلاعاتی جامع  مورد نیاز شهروندان شهرستان کازرون است">
}

<!-- og site name-->
@if (ViewData["MetaOgSiteName"] != null)
{
    <meta property="og:site_name" value="@ViewData["MetaOgSiteName"]">
}
else
{
    <meta property="og:site_name" value="دریملند 2000">
}

<!-- og title-->
@if (ViewData["MetaOgTitle"] != null)
{
    <meta property="og:title" content="@ViewData["MetaOgTitle"]">
}
else
{
    <meta property="og:title" content="دریملند پرتال و رسانه تخصصی شهرستان کازرون">
}

<!-- og url-->
@if (ViewData["MetaOgUrl"] != null)
{
    <meta property="og:url" content="@ViewData["MetaOgUrl"]">
}
else
{
    <meta property="og:url" content="https://dreamland2000.ir/">
}

<meta property="og:type" content="beauty">


<meta property="og:image:type" content="image/jpeg">
<meta property="og:image:width" content="300" />
<meta property="og:image:height" content="400" />

@if (ViewData["MetaOgImage"] != null)
{
    <meta property="og:image" content="@ViewData["MetaOgImage"]">
}
else
{
    <meta property="og:image" content="https://dreamland2000.ir/design/logo/logo01-s.png">
}


@if (ViewData["MetaOgImageUrl"] != null)
{
    <meta property="og:image:url" content="@ViewData["MetaOgImageUrl"]">
}
else
{
    <meta property="og:image:url" content="https://dreamland2000.ir/design/logo/logo01-s.png">
}


@if (ViewData["MetaOgImageAlt"] != null)
{
    <meta property="og:image:alt" content="@ViewData["MetaOgImageAlt"]">
}
else
{
    <meta property="og:image:alt" content="دریملند پرتال و رسانه تخصصی شهرستان کازرون">
}

<!--og description-->
@if (ViewData["MetaOgDescription"] != null)
{
    <meta property="og:description" content="@ViewData["MetaOgDescription"]">
}
else
{
    <meta property="og:description" content="دریملند پرتال و رسانه تخصصی  شهرستان کازرون در حوزه های مختلف شامل  بانک های اطلاعاتی جامع  مورد نیاز شهروندان شهرستان کازرون است">
}

